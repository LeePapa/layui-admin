{
    "name": "thans/layui-admin",
    "description": "layui admin template build",
    "license": "MIT",
    "authors": [
        {
            "name": "Thans",
            "email": "thans@thans.cn"
        }
    ],
    "require": {
        "php": ">=7.0",
        "topthink/framework": "^5.1.10 || ^6.0.0",
        "topthink/think-migration": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "thans\\layuiAdmin\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "thans\\layuiAdmin\\Service"
            ],
            "config": {
                "admin": "src/config/config.php"
            }
        }
    },
    "minimum-stability": "dev"
}